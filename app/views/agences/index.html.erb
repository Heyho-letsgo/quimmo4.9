<h1>Listing agences</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Principal groupe</th>
      <th>Sous groupe</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agences.each do |agence| %>
      <tr>
        <td><%= agence.nom %></td>
        <td><%= agence.principal_groupe %></td>
        <td><%= agence.sous_groupe %></td>
        <td><%= link_to 'Show', agence %></td>
        <td><%= link_to 'Edit', edit_agence_path(agence) %></td>
        <td><%= link_to 'Destroy', agence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agence', new_agence_path %>
